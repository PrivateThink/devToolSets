<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane
        prefHeight="400.0" prefWidth="600.0"
        xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1"
        fx:controller="com.zeros.devtool.controller.index.IndexController">

    <SplitPane dividerPositions="0.3" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
               AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <VBox>
            <TextField fx:id="searchTex" styleClass="search-text"/>
            <TreeView fx:id="rootTree" VBox.vgrow="ALWAYS"/>
        </VBox>
        <BorderPane fx:id="indexPane"/>
    </SplitPane>
</AnchorPane>
